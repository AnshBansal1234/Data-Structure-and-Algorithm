#include<iostream>
using namespace std;
int main()
{
    int t;
    cin>>t;
    while(t--)
    {
        int n;
        cin>>n;
        int a[n];
        for(int i=0;i<n;i++)
            cin>>a[i];
        int key,c=0;
        cin>>key;
        for(int i=0;i<n;i++)
        {
            if(key==a[i])
            {
                cout<<"Present"<<i+1<<endl;
                c=1;
                break;
            }
        }
        if(c==0)
            cout<<"not present"<<endl;
    }
}

